    <!-- MidPage Section -->
    <section id="contact">
      <div class="content">
        <h2>Bize ilet</h2>
        <p>
          Görüş ve önerileriniz bizim için değerlidir.Aşağıdaki formdan bize
          ulaşabilirsiniz
        </p>
      </div>
      <div class="container2">
        <div class="contactInfo">
          <div class="boxc">
            <div class="text">
              <h3 style="text-align: center">Adres</h3>
              <p style="text-align: center; margin-bottom: 2rem; color: white">
                Meşrutiyet, Işık Ünv., 34980 Şile/İstanbul
              </p>
              <iframe
                src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3003.424683855983!2d29.561808515665582!3d41.16890351747333!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x409e32a67119d599%3A0xf4e228611d2df2f0!2zRk1WIEnFn8SxayDDnG5pdmVyc2l0ZXNp!5e0!3m2!1str!2str!4v1612870748823!5m2!1str!2str"
                width="600"
                height="450"
                frameborder="0"
                style="border: 0"
                allowfullscreen=""
                aria-hidden="false"
                tabindex="0"
              ></iframe>
              <h3 style="text-align: center; margin-top: 2rem">Email</h3>
              <p style="text-align: center; color: white">snoll@gmail.com</p>
            </div>
          </div>
        </div>
        <div class="contactForm">
          <form class="form-part" action="/auth/contactus" method="POST">
            <h2>Mesaj Gönder</h2>
            <div class="inputBox">
              <input type="text" name="nameS" required="required" />
              <span>Ad-Soyad</span>
            </div>
            <div class="inputBox">
              <input type="text" name="contactmail" required="required" />
              <span>Email</span>
            </div>
            <div class="inputBox">
              <textarea name="message" required="required"></textarea>
              <span>Mesajını Yaz</span>
            </div>
            <div class="inputBox">
              <input
                class="button"
                type="submit"
                name=""
                value="Mesaj Gönder"
              />
            </div>
          </form>
        </div>
      </div>
    </section>
    <!-- End MidPage Section -->
